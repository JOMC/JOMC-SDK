<module xmlns="http://jomc.org/model"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://jomc.org/model http://jomc.sourceforge.net/model/jomc-1.0.xsd
                            http://jomc.org/sdk/model http://jomc.sourceforge.net/sdk/model/jomc-sdk-1.0.xsd"
        name="${project.name}" version="${project.version}" vendor="${project.organization.name}" modelVersion="1.0">

  <documentation defaultLanguage="en">
    <text language="en">${project.description}</text>
  </documentation>
  <specifications>
    <!-- *********************************************************************************************************** -->
    <specification identifier="XML Schema Set" vendor="${project.organization.name}" version="${xmlschemaset.specification.version}">
      <documentation defaultLanguage="en">
        <text language="en">Set of XML Schemas</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0" mail="schulte2005@users.sourceforge.net"
                createDate="2005-07-25T19:56:45Z"/>

      </authors>
      <properties>
        <property name="schemas" type="org.jomc.sdk.model.SchemasType">
          <documentation defaultLanguage="en">
            <text language="en">List of XML schemas ('schemas' element from XML namespace 'http://jomc.org/sdk/model').</text>
          </documentation>
        </property>
      </properties>
    </specification>
  </specifications>
  <implementations>
    <!-- *********************************************************************************************************** -->
    <implementation identifier="org.jomc.sdk.model.support.SdkModelProcessor"
                    class="org.jomc.sdk.model.support.SdkModelProcessor"
                    classDeclaration="true" name="${project.name}" version="${project.version}"
                    vendor="${project.organization.name}" stateless="true">

      <documentation defaultLanguage="en">
        <text language="en">SDK 'ModelProcessor' implementation.</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0" mail="schulte2005@users.sourceforge.net"
                createDate="2005-07-25T19:56:45Z"/>

      </authors>
    </implementation>
    <!-- *********************************************************************************************************** -->
    <implementation identifier="org.jomc.sdk.model.support.SdkModelValidator"
                    class="org.jomc.sdk.model.support.SdkModelValidator"
                    classDeclaration="true" name="${project.name}" version="${project.version}"
                    vendor="${project.organization.name}" stateless="true">

      <documentation defaultLanguage="en">
        <text language="en">SDK 'ModelValidator' implementation.</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0" mail="schulte2005@users.sourceforge.net"
                createDate="2005-07-25T19:56:45Z"/>

      </authors>
    </implementation>
    <!-- *********************************************************************************************************** -->
    <implementation identifier="Default XML Schema Set" name="Default XML Schema Set" version="${project.version}"
                    vendor="${project.organization.name}">

      <documentation defaultLanguage="en">
        <text language="en">Default XML Schema Set.</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0" mail="schulte2005@users.sourceforge.net"
                createDate="2005-07-25T19:56:45Z"/>

      </authors>
      <properties>
        <property name="schemas" type="org.jomc.sdk.model.SchemasType">
          <documentation defaultLanguage="en">
            <text language="en">List of XML schemas ('schemas' element from XML namespace 'http://jomc.org/sdk/model).</text>
          </documentation>
          <schemas xmlns="http://jomc.org/sdk/model" language-id="http://www.w3.org/2001/XMLSchema"/>
        </property>
      </properties>
    </implementation>
    <!-- *********************************************************************************************************** -->
    <implementation identifier="org.jomc.sdk.model.support.JaxbContextFactory"
                    class="org.jomc.sdk.model.support.JaxbContextFactory"
                    classDeclaration="true" name="${project.name}" version="${project.version}"
                    vendor="${project.organization.name}" stateless="true">

      <documentation defaultLanguage="en">
        <text language="en">XML Schema Set 'JAXBContext' factory implementation.</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0" mail="schulte2005@users.sourceforge.net"
                createDate="2005-07-25T19:56:45Z"/>

      </authors>
      <specifications>
        <reference identifier="javax.xml.bind.JAXBContext"/>
      </specifications>
      <implementations>
        <reference identifier="Default XML Schema Set" version="${project.version}"/>
      </implementations>
    </implementation>
    <!-- *********************************************************************************************************** -->
    <implementation identifier="org.jomc.sdk.model.support.JaxbMarshallerFactory"
                    class="org.jomc.sdk.model.support.JaxbMarshallerFactory"
                    classDeclaration="true" name="${project.name}" version="${project.version}"
                    vendor="${project.organization.name}" stateless="true">

      <documentation defaultLanguage="en">
        <text language="en">XML Schema Set JAXB 'Marshaller' factory implementation.</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0" mail="schulte2005@users.sourceforge.net"
                createDate="2005-07-25T19:56:45Z"/>

      </authors>
      <specifications>
        <reference identifier="javax.xml.bind.Marshaller"/>
      </specifications>
      <implementations>
        <reference identifier="Default XML Schema Set" version="${project.version}"/>
      </implementations>
      <properties>
        <property name="jaxbEncoding" value="UTF-8">
          <documentation defaultLanguage="en">
            <text language="en">See {@link javax.xml.bind.Marshaller#JAXB_ENCODING}.</text>
          </documentation>
        </property>
        <property name="jaxbFormattedOutput" type="boolean" value="false">
          <documentation defaultLanguage="en">
            <text language="en">See {@link javax.xml.bind.Marshaller#JAXB_FORMATTED_OUTPUT}.</text>
          </documentation>
        </property>
        <property name="jaxbFragment" type="boolean" value="true">
          <documentation defaultLanguage="en">
            <text language="en">See {@link javax.xml.bind.Marshaller#JAXB_FRAGMENT}.</text>
          </documentation>
        </property>
        <property name="jaxbSchemaLocation" type="boolean" value="false">
          <documentation defaultLanguage="en">
            <text language="en">See {@link javax.xml.bind.Marshaller#JAXB_SCHEMA_LOCATION}.</text>
          </documentation>
        </property>
        <property name="jaxbNoNamespaceSchemaLocation" value="">
          <documentation defaultLanguage="en">
            <text language="en">See {@link javax.xml.bind.Marshaller#JAXB_NO_NAMESPACE_SCHEMA_LOCATION}.</text>
          </documentation>
        </property>
        <property name="jaxpSchemaValidation" type="boolean" value="false">
          <documentation defaultLanguage="en">
            <text language="en">See {@link javax.xml.bind.Marshaller#setSchema(javax.xml.validation.Schema)}.</text>
          </documentation>
        </property>
      </properties>
    </implementation>
    <!-- *********************************************************************************************************** -->
    <implementation identifier="org.jomc.sdk.model.support.JaxbUnmarshallerFactory"
                    class="org.jomc.sdk.model.support.JaxbUnmarshallerFactory"
                    classDeclaration="true" name="${project.name}" version="${project.version}"
                    vendor="${project.organization.name}" stateless="true">

      <documentation defaultLanguage="en">
        <text language="en">XML Schema Set JAXB 'Unmarshaller' factory implementation.</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0" mail="schulte2005@users.sourceforge.net"
                createDate="2005-07-25T19:56:45Z"/>

      </authors>
      <specifications>
        <reference identifier="javax.xml.bind.Unmarshaller"/>
      </specifications>
      <implementations>
        <reference identifier="Default XML Schema Set" version="${project.version}"/>
      </implementations>
      <properties>
        <property name="jaxpSchemaValidation" type="boolean" value="false">
          <documentation defaultLanguage="en">
            <text language="en">See {@link javax.xml.bind.Unmarshaller#setSchema(javax.xml.validation.Schema)}.</text>
          </documentation>
        </property>
      </properties>
    </implementation>
    <!-- *********************************************************************************************************** -->
    <implementation identifier="org.jomc.sdk.model.support.JaxpSchemaFactory"
                    class="org.jomc.sdk.model.support.JaxpSchemaFactory"
                    classDeclaration="true" name="${project.name}" version="${project.version}"
                    vendor="${project.organization.name}" stateless="true">

      <documentation defaultLanguage="en">
        <text language="en">XML Schema Set JAXP 'Schema' factory implementation.</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0" mail="schulte2005@users.sourceforge.net"
                createDate="2005-07-25T19:56:45Z"/>

      </authors>
      <specifications>
        <reference identifier="javax.xml.validation.Schema"/>
      </specifications>
      <implementations>
        <reference identifier="Default XML Schema Set" version="${project.version}"/>
      </implementations>
    </implementation>
    <!-- *********************************************************************************************************** -->
    <implementation identifier="org.jomc.sdk.model.support.JaxpEntityResolverFactory"
                    class="org.jomc.sdk.model.support.JaxpEntityResolverFactory" classDeclaration="true"
                    name="${project.name}" version="${project.version}" vendor="${project.organization.name}" stateless="true">

      <documentation defaultLanguage="en">
        <text language="en">XML Schema Set JAXP 'EntityResolver' factory implementation.</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0" mail="schulte2005@users.sourceforge.net"
                createDate="2005-07-25T19:56:45Z"/>

      </authors>
      <specifications>
        <reference identifier="org.xml.sax.EntityResolver"/>
      </specifications>
      <implementations>
        <reference identifier="Default XML Schema Set" version="${project.version}"/>
      </implementations>
    </implementation>
    <!-- *********************************************************************************************************** -->
    <implementation identifier="org.jomc.sdk.model.support.JaxpResourceResolverFactory"
                    class="org.jomc.sdk.model.support.JaxpResourceResolverFactory" classDeclaration="true"
                    name="${project.name}" version="${project.version}" vendor="${project.organization.name}" stateless="true">
      <documentation defaultLanguage="en">
        <text language="en">XML Schema Set JAXP 'LSResourceResolver' factory implementation.</text>
      </documentation>
      <authors>
        <author name="Christian Schulte" version="1.0" mail="schulte2005@users.sourceforge.net"
                createDate="2005-07-25T19:56:45Z"/>

      </authors>
      <specifications>
        <reference identifier="org.w3c.dom.ls.LSResourceResolver"/>
      </specifications>
      <implementations>
        <reference identifier="Default XML Schema Set" version="${project.version}"/>
      </implementations>
    </implementation>
  </implementations>
</module>
